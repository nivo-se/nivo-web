{
  "name": "nivo",
  "version": "1.0.0",
  "description": "Nivo - Swedish Company Intelligence Platform - Full Stack Application",
  "private": true,
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "dev:backend": "DATABASE_SOURCE=postgres ./scripts/start_backend.sh",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "setup": "npm run install:frontend && npm run install:backend",
    "deploy": "vercel --prod"
  },
  "workspaces": [
    "frontend"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@vercel/node": "^5.6.3",
    "node-fetch": "^3.3.2",
    "openai": "^6.1.0"
  },
  "overrides": {
    "ajv": "^8.18.0",
    "path-to-regexp": "^6.2.3",
    "undici": "^6.22.1"
  }
}
