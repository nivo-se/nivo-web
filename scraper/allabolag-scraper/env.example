# Database connection string for Drizzle
# Use the non-pooled connection with SSL mode required
DATABASE_URL="postgresql://username:password@host:port/database?sslmode=require"

# Supabase credentials (if needed for client-side auth later)
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key"

# VPN Configuration (for large-scale scraping - DEPRECATED, use Oxylabs instead)
VPN_ENABLED="false"
VPN_PROVIDER="manual"
VPN_ROTATION_STRATEGY="request_count"
VPN_ROTATION_THRESHOLD="1000"
VPN_MAX_REQUESTS_PER_IP="1000"
VPN_MAX_TIME_PER_IP="60"
VPN_CUSTOM_PROXY_URL=""
VPN_API_KEY=""

# Oxylabs Proxy Configuration (for large-scale scraping)
OXYLABS_ENABLED="false"
OXYLABS_USERNAME="user-jason_Z2AvH"  # Your Oxylabs username (without country suffix)
OXYLABS_PASSWORD="your-password"  # Your Oxylabs password
OXYLABS_PROXY_TYPE="datacenter"  # datacenter, residential, or isp
OXYLABS_COUNTRY="us"  # United States (ISO country code: us, se, etc.)
OXYLABS_SESSION_TYPE="rotate"  # rotate (new IP per request) or sticky (same IP for session)
OXYLABS_COUNTRY_IN_USERNAME="true"  # If true, country is added to username: username-country-XX
OXYLABS_PORT="8000"  # Port (8000 for datacenter, 7777 for residential/ISP)









